{
    "name" : "stevenmaguire/trello-php",
    "description" : "PHP wrapper for Trello API",
    "keywords" : ["trello","php","client","wrapper","trello gold"],
    "homepage" : "http://github.com/stevenmaguire/trello-php",
    "license" : "MIT",
    "authors" : [
        {
            "name" : "Steven Maguire",
            "email" : "stevenmaguire@gmail.com",
            "homepage" : "http://stevenmaguire.com",
            "role" : "Developer"
        }
    ],
    "require" : {
        "php": ">=5.4.1",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-hash": "*",
        "ext-openssl": "*",
        "league/oauth1-client": "~1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "mockery/mockery": "0.9.*@dev"
    },
    "autoload": {
        "psr-4": {
            "Trello\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Trello\\Tests\\": "tests/"
        },
        "classmap": [
            "src/",
            "tests/"
        ]
    }
}
